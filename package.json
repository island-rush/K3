{
    "name": "island-rush-v3.3",
    "version": "3.3.1",
    "description": "Island Rush V3 is a Military Strategy Teaching Tool/Game.",
    "main": "./dist/server.js",
    "scripts": {
        "dev": "concurrently \"npm:devBackend\" \"npm:devFrontend\"",
        "lint": "eslint ./src/**/*.ts",
        "test": "jest --config ./jest.config.js",
        "build": "npm run clearBuild && npm run compileBackend && npm run buildClient && npm run moveFrontend",
        "start": "cross-env NODE_ENV=production node -r dotenv/config ./dist/server.js",
        "dev:debug": "concurrently \"npm:devBackend:debug\" \"npm:devFrontend\"",
        "clearBuild": "trash ./dist && trash ./src/react-client/build",
        "devBackend": "ts-node-dev --no-notify -r dotenv/config ./src/server.ts",
        "devFrontend": "cd ./src/react-client && cross-env NODE_OPTIONS=--max_old_space_size=4096 npm start",
        "buildClient": "cd ./src/react-client && npm run build && cd ../..",
        "moveFrontend": "copyfiles -u 1 ./src/react-client/build/**/*.* ./dist",
        "compileBackend": "tsc -p . && copyfiles -u 3 ./src/server/pages/**/*.html ./dist/server/pages",
        "devBackend:debug": "ts-node-dev --no-notify -r dotenv/config --inspect -- ./src/server.ts",
        "typecheckFrontend": "cd ./src/react-client && tsc"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/island-rush/K3.git"
    },
    "author": {
        "name": "Spencer Adolph",
        "email": "spenceradolph@ymail.com",
        "url": "https://www.spenceradolph.com"
    },
    "license": "closed",
    "bugs": {
        "url": "https://github.com/island-rush/K3/issues"
    },
    "homepage": "https://github.com/island-rush/K3#readme",
    "devDependencies": {
        "@types/connect-azuretables": "^1.0.0",
        "@types/connect-redis": "0.0.13",
        "@types/express": "^4.17.2",
        "@types/express-session": "^1.15.16",
        "@types/express-socket.io-session": "^1.3.2",
        "@types/jest": "^24.0.25",
        "@types/md5": "^2.1.33",
        "@types/mysql2": "github:types/mysql2",
        "@types/node": "^12.12.17",
        "@types/redis": "^2.8.14",
        "@types/redux": "^3.6.0",
        "@types/socket.io": "^2.1.4",
        "@types/socket.io-redis": "^1.0.25",
        "@typescript-eslint/eslint-plugin": "^2.12.0",
        "concurrently": "^5.0.1",
        "copyfiles": "^2.1.1",
        "cross-env": "^6.0.3",
        "dotenv": "^8.2.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-typescript": "^6.3.1",
        "eslint-plugin-import": "^2.19.1",
        "jest": "^24.9.0",
        "trash-cli": "^3.0.0",
        "ts-jest": "^24.3.0",
        "ts-node-dev": "^1.0.0-pre.44",
        "typescript": "^3.7.3"
    },
    "dependencies": {
        "connect-azuretables": "^1.0.20",
        "connect-loki": "^1.1.0",
        "connect-redis": "^4.0.4",
        "express": "^4.17.1",
        "express-session": "^1.17.0",
        "express-socket.io-session": "^1.3.5",
        "md5": "^2.2.1",
        "mysql2": "^2.0.2",
        "redis": "^2.8.0",
        "socket.io": "^2.4.0",
        "socket.io-redis": "^5.2.0"
    }
}
